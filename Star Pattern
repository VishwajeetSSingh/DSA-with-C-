#include<iostream.h>
using namespace std;
void print2(int n) {
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < i; j++)
        {
            cout<<"* ";
        }
    }
}
int main() {
    int i;
    cin>>t;
    for (int i = 0; i < t; i++)
    {
        int n;
        cin >> n;
        print2(n);
    }
    
}
